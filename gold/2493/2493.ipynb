   "source": [
    "N=int(input())\n",
    "number=list(map(int,input().split()))\n",
    "result=[0]*N\n",
    "stack=[]\n",
    "for i in range(N-1,-1,-1) :\n",
    "  while stack and number[stack[-1]] < number[i] :\n",
    "    result[stack.pop()]=i+1\n",
    "  stack.append(i)\n",
    "print(*result)"
   ]
  }
