   "source": [
    "T=int(input())\n",
    "for t in range(T):\n",
    "  N=int(input())\n",
    "  board=[list(map(int,input()))for _ in range(N)]\n",
    "  score=0\n",
    "  mid=N//2\n",
    "  for i in range(mid):\n",
    "    score+=sum(board[i][mid-i:mid+i+1])\n",
    "    score+=sum(board[N-1-i][mid-i:mid+i+1])\n",
    "  score+=sum(board[mid][:])\n",
    "  print(f\"#{t+1} {score}\")\n"
   ]
  }
