    "\n",
    "T = int(input())\n",
    "for _ in range(T):\n",
    "    alpha = deque(input())\n",
    "    k = int(input())\n",
    "    k_list = list(map(int, input().split()))\n",
    "\n",
    "    for num in k_list:\n",
    "        alpha.rotate(-num)  # rotate 메서드로 한 번에 회전\n",
    "    print(''.join(alpha))\n"
   ]
